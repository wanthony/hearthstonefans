<SCRIPT language="javascript" type="text/javascript">
function swapbg(item){
 if(document.getElementById(item).checked){
  document.getElementById(item+'bg').style.background='#efcd94'
 }
 else{
  document.getElementById(item+'bg').style.background=''
 }
}
</SCRIPT>
